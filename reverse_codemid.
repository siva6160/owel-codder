def reverse(x):
        s=abs(x)
        tem=s
        rev=0
        while s>0:
            r=s%10
            rev=rev*10+r
            s=s//10
        if rev>=2147483647:
            return 0
        if x<0:
            return -(rev)
        return rev
x=int(input())
print(reverse(x))
        
        
