#include<stdio.h>
int main(){
    int t;
    scanf("%d",&t);
    for (int i=0;i<t;i++){
        int n;
        scanf("%d",&n);
        int temp=n;
        int rev=0;
        while (n>0){
            int r=n%10;
            rev=rev*10+r;
            n=n/10;
        }
        if(temp==rev){
            printf("True\n");
        }
        else{
            printf("False\n");
            
        }
    }
return 0;
}
