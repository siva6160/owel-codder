#include<stdio.h>
#include<math.h>
int main(){
    int n,c=0;
    scanf("%d",&n);
    //int s=sqrt(n);
    for(int i=1;i*i<=n;i++){
        if (i*i==n){
            c=1;
            break;
           
        }
    }
    if(c==1){
        printf("True");
    }
    else{
        printf("False");
    }
return 0;  
}
