nt isPrime(int N){
        // code here
        int c=0;
        if (N<=1){
            return 0;
        }
        for(int i=2;i*i<=N;i++){
            if (N%i==0){
                c=1;
                break;
                
            }
        }
        if (c==0){
            return 1;
        }
        else{
            return 0;
        }
    }
};
